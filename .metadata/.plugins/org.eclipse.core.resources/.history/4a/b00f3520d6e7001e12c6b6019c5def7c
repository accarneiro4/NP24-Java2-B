package br.com.senaisp.bauru.amandacaroline.licao04.secao03;

public class ExemploString01 {
	
	public static void main (String[] args) {
		String nome = "Amanda";
		String nome2 = "Amanda";
		if (nome == nome2) {
			System.out.println("São iguais");
		} else {
			System.out.println("São diferentes");
		}
	}

}
